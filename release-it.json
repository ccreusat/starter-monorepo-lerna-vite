{
    "git": {
        "requireBranch": "release-it",
        "requireCleanWorkingDir": false,
        "push": true,
        "tagName": "${packageName}-v${version}",
        "commitsPath": ".",
        "commitMessage": "chore: release v${version}",
        "requireCommits": true,
        "requireCommitsFail": false
    },
    "github": {
        "preRelease": true,
        "releaseName": "${packageName}-v${version}"
    },
    "npm": {
        "publish": true
    }
}