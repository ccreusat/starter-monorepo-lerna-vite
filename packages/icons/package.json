{
  "name": "@starter-ccreusat/icons",
  "version": "0.0.15",
  "private": false,
  "keywords": [
    "react",
    "frontend",
    "components",
    "icons"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ccreusat/starter-monorepo-lerna-vite.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm icons && vite build",
    "icons": "npx @svgr/cli assets --config-file ./svgr.config.cjs",
    "lint": "eslint -c ../../config/eslint/icons.json .",
    "lint:fix": "eslint -c ../../config/eslint/icons.json --fix .",
    "pretty": "pnpm pretty:write && pnpm pretty:check",
    "pretty:check": "pnpm prettier --check .",
    "pretty:write": "pnpm prettier --write ."
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@svgr/cli": "^6.4.0",
    "@types/node": "^18.7.16",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "4.0.4",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "4.4.9",
    "vite-plugin-dts": "3.5.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "gitHead": "495efa78655005f16245bac5e4fa9a1d59f4fdb4"
}
